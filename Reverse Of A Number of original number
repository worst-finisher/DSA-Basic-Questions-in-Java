import java.util.*;
public class Main 
{
    public static void main(String args[])
    {
        long lastDigit;
        Scanner sc = new Scanner(System.in);
        long n = sc.nextLong();
        long rev = 0;
        while(n>0)
        {
            lastDigit = n%10;
            rev = ((rev*10)+lastDigit);
            n = n/10;
        }
            System.out.print(rev);
    }
}
