import java.util.*;
public class Main
{
    public static boolean prime(int n)
    {
        boolean prime = true;
        for(int i=2;i<n;i++)
        {
            if(n%i==0)
            {
             prime = false;
                break;
            }
        }
        return prime;
    }
    public static void main(String arg[])
    {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        System.out.println(prime(n));
    }
}
