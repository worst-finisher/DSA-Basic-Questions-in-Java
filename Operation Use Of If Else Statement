import java.util.*;
public class Main 
{
    public static void main(String args[])
    {
        Scanner sc = new Scanner(System.in);
        int age = sc.nextInt();
        {
            if(age>=18)
            {
                System.out.print("Adult Person And Eligible For Both VOTING & DRIVING");
            }
            else
            {
                System.out.print("Not A Adult Person And Not Eligible For VOTING & DRIVING");
            }
        }
    }
}
