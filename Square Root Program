import java.util.*;
public class Main
{
    public static float sqrt(int n)
    {
        float r = (float)Math.sqrt(n);
        return r;
    }
    public static void main(String arg[])
    {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        System.out.println(sqrt(n));
    }
}
