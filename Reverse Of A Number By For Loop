import java.util.*;
public class Main 
{
    public static void main(String args[])
    {
        int lastDigit;
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();    
        for(;n>0;n = n/10)
        {
            lastDigit = n%10;
            System.out.print(lastDigit);
        }
    }
}
