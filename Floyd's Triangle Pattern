import java.util.*;
public class Main 
{
    public static void hp(int n)
    {
        int i,j,count=1;
        for(i=1;i<=n;i++)
        {
            for(j=1;j<=i;j++)
            {
                System.out.print(count+" ");
                count++;
            }
            System.out.println();
        }
    }
    public static void main(String arg[])
    {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        System.out.println();
        hp(n);
    }
}
