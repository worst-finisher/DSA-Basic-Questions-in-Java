import java.util.*;
public class Main 
{
    public static int getlarger(int num[])
    {
        int larger = Integer.MIN_VALUE;
        for(int i=0;i<num.length;i++)
        {
            if(larger<num[i])
            {
                larger=num[i];
            }
        }
        return larger;
    }
    public static void main(String arg[])
    {
        int num[]={1,8,6,7,3,9,10,5,16,0,2};//Here We Can Also Take Any Input Or Values For Num .
        int getlarger = getlarger(num);
        System.out.print("The Largest Number Of The Given Array = "+ getlarger);
    }
}
