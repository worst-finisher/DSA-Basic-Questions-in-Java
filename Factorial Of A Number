import java.util.*;
public class Main 
{
    public static int fact(int n)
    {
        int f = 1,i;
        for(i=1;i<=n;i++)
        {
            f = f*i;   
        }
        return f;
    }
    public static void main(String arg[])
    {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        System.out.println(fact(n));
    }
}
