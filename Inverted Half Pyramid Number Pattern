import java.util.*;
public class Main 
{
    public static void hpip(int n)
    {
        int i,j;
        for(i=1;i<=n;i++)
        {
            for(j=1;j<=n-i+1;j++)
            {
                System.out.print(j);
            }
        System.out.println();
        
        }
    
    }
    public static void main(String arg[])
    {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        hpip(n);
    }
}
