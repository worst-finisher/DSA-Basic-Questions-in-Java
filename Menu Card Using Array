import java.util.*;
public class Main 
{
    public static String Res(String menu[],int key)
    {
        for(int i=0;i<menu.length;i++)
        {
            if(i==key)
            {
                return menu[i];
            }
        }
        return "0";
    }
    public static void main(String arg[])
    {
        System.out.println("***********************************************************");
        System.out.println("******************* Jung Food Menu Card *******************");
        System.out.println("***********************************************************");
        System.out.println("*** --> Press 0 For Maggie ğŸ˜‹                           ***");
        System.out.println("*** --> Press 1 For Pizza ğŸ˜‹                            ***");
        System.out.println("*** --> Press 2 For Burger ğŸ˜‹                           ***");
        System.out.println("*** --> Press 3 For Chowmein ğŸ˜‹                         ***");
        System.out.println("*** --> Press 4 For Pasta ğŸ˜‹                            ***");
        System.out.println("*** --> Press 5 For Adding Cheese in Maggie ğŸ˜‹ ğŸ˜       ***");
        System.out.println("*** --> Press 6 For Adding Cheese in Pizza ğŸ˜‹ ğŸ˜        ***");
        System.out.println("*** --> Press 7 For Adding Cheese in Burger ğŸ˜‹ ğŸ˜       ***");
        System.out.println("*** --> Press 8 For Adding Cheese in Chowmein ğŸ˜‹ ğŸ˜     ***");
        System.out.println("*** --> Press 9 For Adding Cheese in Pasta ğŸ˜‹ ğŸ˜        ***");
        System.out.println("***********************************************************");
        System.out.println("***********************************************************");
        System.out.println();
        Scanner sc = new Scanner(System.in);
        int key = sc.nextInt();
        String menu[]={"Maggie","Pizza","Burger","Chowmein","Pasta","Cheese Maggie","Cheese Pizza","Cheese Burger","Cheese Chowmein","Cheese Pasta"};
        String order = Res(menu,key);
        if(order!="0")
        {
            System.out.println("Ok Sir You Order "+order);
            System.out.println(" Please Wait For Some Time ğŸ˜‰ ğŸ˜‰");
        }
        else
        {
            System.out.print("ğŸ™‚ ERROR 404 : Dish Not Found ğŸ™‚");
        }
    }
}
