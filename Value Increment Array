import java.util.*;
public class Main 
{
    public static void upgrade(int marks[])
    {
        int i;
        for(i=0;i<marks.length;i++)
        {
            marks[i]=marks[i]+1;
        }
    }
    public static void main(String arg[])
    {
        int marks[] = {1,2,3,4};
        upgrade(marks);
        for(int i=0;i<marks.length;i++)
        {
            System.out.print(marks[i]);
        }
    }
}
